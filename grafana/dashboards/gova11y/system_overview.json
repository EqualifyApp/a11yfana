          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH table_info AS (\n  SELECT\n    table_name AS \"Table\",\n    COUNT(*) AS \"Column Count\"\n  FROM\n    information_schema.columns\n  WHERE\n    table_catalog = 'a11ydata'\n    AND table_schema = '${db_schemas}'\n  GROUP BY\n    table_name\n),\nrow_counts AS (\n  SELECT\n    table_name,\n    (xpath('/row/cnt/text()', xml_count))[1]::text::bigint as \"Row Count\"\n  FROM (\n    SELECT\n      table_name,\n      table_schema,\n      query_to_xml('SELECT count(*) as cnt FROM ' || quote_ident(table_schema) || '.' || quote_ident(table_name), false, true, '') as xml_count\n    FROM\n      information_schema.tables\n    WHERE\n      table_catalog = 'a11ydata'\n      AND table_schema = '${db_schemas}'\n  ) as sub_query\n)\nSELECT\n  ti.\"Table\",\n  ti.\"Column Count\",\n  rc.\"Row Count\"\nFROM\n  table_info ti\nJOIN row_counts rc ON ti.\"Table\" = rc.table_name\nORDER BY\n  ti.\"Table\";\n",
          "refId": "A",
          "sql": {
            "columns": [
