      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "SCi_YTYVz"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  td.\"domain\" AS \"Domain\",        \n  tu.url AS \"URL\",             \n  ar.scan_id AS \"Scan id\",         \n  -- ar.rule_type,       \n  ar.axe_id AS \"axe id\",          \n  ar.impact AS \"Impact\", \n\n  CONCAT(\n    ROUND(EXTRACT(EPOCH FROM (now() - asd.created_at)) / 60), \n    ' minutes ago'\n  ) AS \"Time since created\"  \n\n  -- asd.created_at       \nFROM \n  axe.rules AS ar\nJOIN\n  axe.scan_data AS asd ON asd.id = ar.scan_id\nJOIN\n  targets.urls AS tu ON tu.id = asd.url_id\nJOIN \n  targets.domains AS td ON td.id = tu.domain_id\nWHERE \n  ar.rule_type = 'violations' AND \n  ar.id IN (\n    SELECT DISTINCT r.id\n    FROM \n      axe.rules AS r,\n      jsonb_array_elements_text(r.tags) AS tag\n    WHERE \n      r.rule_type = 'violations' AND \n      tag LIKE '%wcag%'\n  )\n  AND asd.created_at >= $__timeFrom() AND asd.created_at <= $__timeTo()\n  ORDER BY asd.created_at DESC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Axe Summary | WCAG Violations Only",
      "type": "table"
